pragma solidity ^0.8.13;

contract CodeSize {
    /**
     * The challenge is to create a contract whose runtime code (bytecode) size is greater than 1kb but less than 4kb
     */

uint256 public a1;
    uint256 public a2;
    uint256 public a3;
    uint256 public a4;
    uint256 public a5;
    uint256 public a6;
    uint256 public a7;
    uint256 public a8;
    uint256 public a9;
    uint256 public a10;
    uint256 public a11;
    uint256 public a12;
    uint256 public a13;
    uint256 public a14;
    uint256 public a15;
    uint256 public a16;
    uint256 public a17;
    uint256 public a18;
    uint256 public a19;
    uint256 public a20;

    // Redundant functions to increase code size
    function setA1(uint256 _a1) public { a1 = _a1; }
    function setA2(uint256 _a2) public { a2 = _a2; }
    function setA3(uint256 _a3) public { a3 = _a3; }
    function setA4(uint256 _a4) public { a4 = _a4; }
    function setA5(uint256 _a5) public { a5 = _a5; }
    function setA6(uint256 _a6) public { a6 = _a6; }
    function setA7(uint256 _a7) public { a7 = _a7; }
    function setA8(uint256 _a8) public { a8 = _a8; }
    function setA9(uint256 _a9) public { a9 = _a9; }
    function setA10(uint256 _a10) public { a10 = _a10; }
    function setA11(uint256 _a11) public { a11 = _a11; }
    function setA12(uint256 _a12) public { a12 = _a12; }
    function setA13(uint256 _a13) public { a13 = _a13; }
    function setA14(uint256 _a14) public { a14 = _a14; }
    function setA15(uint256 _a15) public { a15 = _a15; }
    function setA16(uint256 _a16) public { a16 = _a16; }
    function setA17(uint256 _a17) public { a17 = _a17; }
    function setA18(uint256 _a18) public { a18 = _a18; }
    function setA19(uint256 _a19) public { a19 = _a19; }
    function setA20(uint256 _a20) public { a20 = _a20; }
}
